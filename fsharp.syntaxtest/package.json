{
  "name": "FSharp.SyntaxTool",
  "version": "0.0.0",
  "publisher": "Ionide",
  "description": "Dev Tool for Quick Syntax Grammar Testing",
  "categories": [
    "Languages"
  ],
  "icon": "images/logo.png",
  "homepage": "http://ionide.io",
  "contributors": [
    {
      "name": "Jared Hester",
      "email": "jared.hester@gmail.com",
      "url": "https://twitter.com/cloudRoutine"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ionide/ionide-fsgrammar.git"
  },
  "engines": {
    "vscode": "^0.10.0"
  },
  "main": "./syntaxtool",
  "contributes": {
    "languages": [
      {
        "id": "fsharp",
        "aliases": [ "F# Syntax" ],
        "extensions": [ ".fs", ".fsx", ".fsi" ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fsharp",
        "scopeName": "source.fsharp",
        "path": "./syntaxes/fsharp.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp.fsx",
        "path": "./syntaxes/fsharp.fsx.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp.fsi",
        "path": "./syntaxes/fsharp.fsi.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp.fsl",
        "path": "./syntaxes/fsharp.fsl.json"
      }
    ],
    "commands": [
      {
        "command": "syntaxTools.TokenizeFile",
        "title": "Get Textmate tokens for active editor"
      }
    ],
    "outputChannels": [
      "Textmate Tokens"
    ]
  },
  "activationEvents": [
    "onLanguage:fsharp",
    "onCommand:syntaxTools.TokenizeFile"
  ],
  "extensionDependencies": [
    "vscode.fsharp"
  ],
  "scripts": {
    "build": "fable ../src/FSharp.SyntaxTools.fsproj -s -o ../fsharp.syntaxtest -m commonjs --verbose"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "fable-core": "^0.5.4",
    "vscode-textmate": "2.1.1"    
  },
  "devDependencies": {
    "vscode": "^0.11.17",
    "fable-compiler": "^0.5.6"
  }
}
